{
  "[ruby]": {
    "editor.formatOnSave": false
  },
  "[rust]": {
    "editor.formatOnSave": false
  },
  "breadcrumbs.enabled": true,
  "editor.fontLigatures": true,
  "editor.codeLens": false,
  "editor.tabSize": 2,
  "editor.fontFamily": "IBM Plex Mono",
  "editor.fontSize": 14,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Italic overrides",
        "scope": [
          "comment",
          "variable.parameter",
          "constant.language.symbol.hashkey.parameter.function.ruby",
          "variable.language.self.ruby",
          "entity.other.attribute-name.js.jsx",
          "variable.language.this.js",
          "text.html.basic entity.other.attribute-name.html"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Italic exceptions",
        "scope": [
          "entity.name.type.instance.jsdoc",
          "storage.type.class.jsdoc"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.minimap.enabled": false,
  "editor.formatOnSave": false,
  "editor.insertSpaces": true,
  "extensions.ignoreRecommendations": false,
  "files.insertFinalNewline": true,
  "githubPullRequests.hosts": [
    {
      "host": "https://github.com",
      "username": "oauth",
      "token": "system"
    }
  ],
  "gitlens.keymap": "alternate",
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "rust.jobs": 4,
  "rust.goto_def_racer_fallback": true,
  "rust.clippy_preference": "on",
  "rust-client.channel": "nightly",
  "terminal.integrated.fontSize": 14,
  "vim.enableNeovim": true,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["\\"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.commentLine",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "j"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.formatSelection",
          "args": []
        }
      ]
    }
  ],
  "vim.disableExtension": false,
  "vim.leader": ",",
  "vim.hlsearch": true,
  "vim.neovimPath": "nvim",
  "vim.showcmd": true,
  "vim.ignorecase": true,
  "vim.visualstar": true,
  "vim.incsearch": true,
  "vim.sneak": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "c"],
      "after": [],
      "commands": [
        {
          "command": ":nohl",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "j"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.formatDocument",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "w"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.trimTrailingWhitespace",
          "args": []
        },
        {
          "command": "workbench.action.files.save",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "f"],
      "after": [],
      "commands": [
        {
          "command": "workbench.files.action.showActiveFileInExplorer",
          "args": []
        }
      ]
    },
    {
      "before": ["\\"],
      "after": [],
      "commands": [
        {
          "command": "editor.action.commentLine",
          "args": []
        }
      ]
    }
  ],
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Gruvbox Dark (Hard)",
  "workbench.fontAliasing": "antialiased",
  "workbench.sideBar.location": "left",
  "workbench.statusBar.feedback.visible": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.startupEditor": "newUntitledFile",
  "window.titleBarStyle": "native"
}

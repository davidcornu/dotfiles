// Place your key bindings in this file to overwrite the defaults
[
  {
    "key": "cmd+e",
    "command": "emmet.expandAbbreviation"
  },
  {
    "key": "pagedown",
    "command": "scrollPageDown",
    "when": "terminalFocus"
  },
  {
    "key": "pageup",
    "command": "scrollPageUp",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+shift+j",
    "command": "workbench.action.toggleMaximizedPanel"
  }
]
